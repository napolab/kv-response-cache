{
  "name": "@napolab/kv-response-cache",
  "version": "0.2.1",
  "private": false,
  "description": "Hono middleware for response caching using Cloudflare's KV storage",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "npm-run-all -p lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fmt": "npm-run-all -s fmt:*",
    "fmt:eslint": "eslint --fix .",
    "fmt:prettier": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "release": "npm-run-all -s release:*",
    "release:build": "npm run build",
    "release:publish": "changeset publish"
  },
  "author": "@naporin0624",
  "license": "MIT",
  "overrides": {
    "tsconfig-paths": "^4.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@cloudflare/workers-types": "^4.20230914.0",
    "@naporin0624/eslint-config": "^0.13.1",
    "@tsconfig/esm": "^1.0.4",
    "@tsconfig/node18": "^2.0.1",
    "@tsconfig/recommended": "^1.0.3",
    "@vitest/coverage-v8": "^0.34.5",
    "eslint": "^8.49.0",
    "hono": "^3.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.5",
    "vitest-environment-miniflare": "^2.14.1"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.5"
  },
  "peerDependencies": {
    "hono": ">=3.*"
  },
  "keywords": [
    "hono",
    "middleware",
    "cache",
    "response cache",
    "cloudflare",
    "kv",
    "cloudflare kv"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/napolab/kv-response-cache.git"
  },
  "bugs": {
    "url": "https://github.com/napolab/kv-response-cache/issues"
  },
  "files": [
    "README",
    "dist",
    "package.json"
  ]
}
