{
  "name": "@napolab/kv-response-cache",
  "version": "0.1.0",
  "private": false,
  "description": "Hono middleware for response caching using Cloudflare's KV storage",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "NODE_ENV=development rollup -c rollup.config.dev.mjs -w",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "start": "NODE_ENV=production node dist/index.mjs",
    "lint": "npm-run-all -p lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fmt": "npm-run-all -s fmt:*",
    "fmt:eslint": "eslint --fix .",
    "fmt:prettier": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules vitest",
    "publish": "changeset publish"
  },
  "author": "@naporin0624",
  "license": "MIT",
  "overrides": {
    "tsconfig-paths": "^4.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@naporin0624/eslint-config": "^0.11.0",
    "@rollup/plugin-run": "^3.0.1",
    "@tsconfig/esm": "^1.0.3",
    "@tsconfig/node18": "^2.0.0",
    "@tsconfig/recommended": "^1.0.2",
    "deepmerge": "^4.3.1",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "hono": "^3.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rollup": "^3.21.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "rollup-plugin-tsconfig-paths": "^1.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0",
    "vitest-environment-miniflare": "^2.14.0"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.5"
  },
  "peerDependencies": {
    "hono": ">=3.*"
  },
  "keywords": [
    "hono",
    "middleware",
    "cache",
    "response cache",
    "cloudflare",
    "kv",
    "cloudflare kv"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/napolab/kv-response-cache.git"
  },
  "bugs": {
    "url": "https://github.com/napolab/kv-response-cache/issues"
  },
  "files": [
    "README",
    "index.mjs",
    "index.mjs.map",
    "index.cjs",
    "index.cjs.map",
    "index.d.ts"
  ]
}
